{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "react boilerplate project",
  "keywords": [
    "react",
    "webpack3"
  ],
  "main": "index.js",
  "engines": {
    "npm": ">=5.0.0",
    "node": ">=8.0.0"
  },
  "scripts": {
    "dev": "better-npm-run dev",
    "lint-js": "npx eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" ."
  },
  "betterScripts": {
    "dev": {
      "command": "babel-node --presets=env,stage-0 --plugins=transform-runtime,add-module-exports ./tools/dev.js",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "nodemonConfig": {
    "ignore": [
      "dist/*",
      "src/*",
      ".git",
      "node_modules/**/node_modules"
    ],
    "exec": "babel-node --presets=env,stage-0 --plugins=transform-runtime,add-module-exports ./server/app.js",
    "delay": "2000",
    "watch": [
      "server"
    ],
    "env": {
      "NODE_ENV": "development"
    }
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "jquery": "^1.12.4",
    "koa": "^2.3.0",
    "moment": "^2.18.1",
    "prop-types": "^15.5.8",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.3",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "better-npm-run": "^0.1.0",
    "clean-webpack-plugin": "^0.1.16",
    "concurrently": "^3.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.5",
    "cssgrace": "^3.0.0",
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-css-modules": "^2.7.5",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^7.3.0",
    "extend": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "prettier": "^1.6.1",
    "progress-bar-webpack-plugin": "^1.10.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.5",
    "webpack-build-notifier": "^0.1.16"
  },
  "author": "Alan Lee",
  "license": "ISC"
}
